
@{
    ViewData["Title"] = "Category List";
}
<h1>@ViewData["Title"]</h1>

@model List<Category>


<div class="container">
    <table class="table table-bordered table-striped" >
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }

        </tbody>
        
    </table>
</div>

